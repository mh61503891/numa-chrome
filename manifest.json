{
  "name": "numa-chrome",
  "description": "numa-chrome",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "Masayuki Higashino",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "css": ["content.css"],
    "js": ["content.js"],
    "run_at": "document_end"
  }],
  "web_accessible_resources": [
    "numakuro.png"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
